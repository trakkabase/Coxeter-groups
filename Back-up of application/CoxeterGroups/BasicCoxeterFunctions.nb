       RowBox[{"M", ",", 
        RowBox[{"{", 
         RowBox[{"s1", ",", "s2", ",", "..."}], "}"}]}], "]"}], " ", "but", 
      " ", "assumes", " ", "a", " ", "priori", " ", "that", " ", 
      RowBox[{"{", 
       RowBox[{"s1", ",", "s2", ",", "..."}], "}"}], " ", "all", " ", "lie", 
      " ", "in", " ", "the", " ", "same", " ", "connected", " ", 
