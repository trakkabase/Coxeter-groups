Cell[BoxData[{
 RowBox[{
  RowBox[{
   RowBox[{"EuclideanCGQ", "::", "usage"}], "=", 
   "\"\<EuclideanCGQ[M] returns True if W(M) is a Euclidean Coxeter group, ie \
it acts discretely by isometries on some Euclidean space, and False \
otherwise.\>\""}], ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{
   RowBox[{"SphericalCGQ", "::", "usage"}], "=", 
   "\"\<SphericalCGQ[M] returns True if W(M) is finite, and False \
otherwise.\>\""}], ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{
   RowBox[{"HyperbolicCGQ", "::", "usage"}], "=", 
   "\"\<HyperbolicCGQ[M] returns True if W(M) is a hyperbolic Coxeter group, \
ie the signature of its bilinear form is (n-1,0,1), and False \
otherwise.\>\""}], ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{
   RowBox[{"EvenCGQ", "::", "usage"}], "=", 
   "\"\<EvenCGQ[M] returns True if every edge lable in the Coxeter diagram of \
type M is even, and False otherwise.\>\""}], ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{
   RowBox[{"RACGQ", "::", "usage"}], "=", 
   "\"\<RACGQ[M] returns True if M corresponds to a right-angled Coxeter \
system, and False otherwise.\>\""}], ";"}]}], "Input",
 CellGroupingRules->{"GroupTogetherGrouping", 10003.},
