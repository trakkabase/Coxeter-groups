Cell[BoxData[{
 RowBox[{
  RowBox[{
   RowBox[{"TitsM1", "::", "usage"}], "=", 
   "\"\<TitsM1[w] applies all possible Tits moves of type M1 to w.\>\""}], 
  ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{
   RowBox[{"TitsM2", "::", "usage"}], "=", 
   "\"\<TitsM2[M,w] produces a list of all words accessible from w by up to \
one Tits move of type M2 in the Coxeter system accociated to M.\n\
TitsM2[W,wList] produces a list of all words accessible from any word in \
wList by up to one Tits move of type M2 in the Coxeter system accociated to \
M.\>\""}], ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{
   RowBox[{"CoxeterReduce", "::", "usage"}], "=", 
   "\"\<CoxeterReduce[M,w] produces a list of all possible reduced words \
representing w in the Coxeter system associated to M.\>\""}], 
  ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{
   RowBox[{"FindCoxeterReducedWord", "::", "usage"}], "=", 
   "\"\<FindCoxeterReducedWord[M,w] produces some reduced word representing w \
in the Coxeter system associated to M.\>\""}], ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{
   RowBox[{"TitsWordProblem", "::", "usage"}], "=", 
   "\"\<TitsWordProblem[M, w, v] returns true if w and v are words \
representing the same element in the Coxeter system associated to M, False \
otherwie.\>\""}], ";"}]}], "Input",
 CellChangeTimes->{{3.8550830276990986`*^9, 3.8550830580928726`*^9}, {
   3.855083183329112*^9, 3.8550831833937073`*^9}, {3.855083217208041*^9, 
