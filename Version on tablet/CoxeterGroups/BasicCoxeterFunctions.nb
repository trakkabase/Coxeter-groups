Cell[CellGroupData[{

Cell["Generators and word manipulations", "Subsubsection",
 CellChangeTimes->{{3.8555794779536767`*^9, 
  3.8555794852907543`*^9}},ExpressionUUID->"41191a47-6d77-494f-88fa-\
fddd7d49b451"],

Cell[BoxData[{
 RowBox[{
  RowBox[{
   RowBox[{"Generators", "::", "usage"}], "=", 
   "\"\<Generators[M] gives an ordered list of the Coxeter generators of the \
Coxeter system with matrix M, each generator being of the form \\\"si\\\" for \
some integer i.\>\""}], ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{
   RowBox[{"GeneratorQ", "::", "usage"}], "=", 
   "\"\<GeneratorQ[\\\"word\\\"] returns True if \\\"word\\\" is a single \
s-symble, eg \\\"s123\\\", and False otherwise.\>\""}], 
  ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{
   RowBox[{"GeneratorIndex", "::", "usage"}], "=", 
   "\"\<GeneratorIndex[s] returns the integer index of the generators s.\n\
GeneratorIndex[sList] returns a list of the integer indices of the generators \
in sList.\>\""}], ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"GeneratorSort", "::", "usage"}], "=", 
  "\"\<GeneratorSort[sList] sorts the list of generator sList by their \
indices.\>\""}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{
   RowBox[{"CoxeterWordRewrite", "::", "usage"}], "=", 
   "\"\<CoxeterWordRewrite[w, rules] rewrites the word w using the \
rules.\>\""}], ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{
   RowBox[{"DisplayGenerator", "::", "usage"}], "=", 
   "\"\<DisplayGenerator[s] outputs the generator s in a human-friendly form \
using subscripts.\>\""}], ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{
   RowBox[{"DisplayWord", "::", "usage"}], "=", 
   "\"\<DisplayWord[w] outputs the word w in a human-friendly form using \
subscripts.\>\""}], ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{
   RowBox[{"WordLength", "::", "usage"}], "=", 
   "\"\<WordLength[w] gives the number of generators appearing in the word w.\
\>\""}], ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{
   RowBox[{"WordInverse", "::", "usage"}], "=", 
   "\"\<WordInverse[w] gives the word resulting from reversing the order of \
the generators in w.\>\""}], ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{
   RowBox[{"WordTake", "::", "usage"}], "=", 
   "\"\<WordTake[\\\"word\\\", n] returns the first n s-symbols from \\\"word\
\\\".\nWordTake[\\\"word\\\", -n] returns the last n s-symbols from \
\\\"word\\\".\nWordTake[\\\"word\\\", {n}] returns the n-th s-symbol from \
\\\"word\\\".\nWordTake[\\\"word\\\", {m, n}] returns the m-th through n-th \
s-symbols from \\\"word\\\".\>\""}], ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{
   RowBox[{"WordDrop", "::", "usage"}], "=", 
   "\"\<WordDrop[\\\"word\\\", n] returns \\\"word\\\" with the first n \
s-symbols removed.\nWordDrop[\\\"word\\\", -n] returns \\\"word\\\" with the \
last n s-symbols removed.\nWordDrop[\\\"word\\\", {n}] returns \\\"word\\\" \
with the n-th s-symbol removed.\nWordDrop[\\\"word\\\", {m, n}] returns \
\\\"word\\\" with the m-th through n-th s-symbols removed.\>\""}], ";", 
  "\"\<.\>\"", ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{
   RowBox[{"WordSymbols", "::", "usage"}], "=", 
   "\"\<WordSymbols[\\\"word\\\"] returns an ordered list of all distinct \
s-symbols appearing in \\\"word\\\".\>\""}], ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{
   RowBox[{"WordPalindromeQ", "::", "usage"}], "=", 
   "\"\<WordPalindromeQ[\\\"word\\\"] returns True if \
WordInverse[\\\"word\\\"] is identical to \\\"word\\\", and False \
otherwise.\>\""}], ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{
   RowBox[{"CentralGenerator", "::", "usage"}], "=", 
   "\"\<CentralGenerator[\\\"word\\\"] returns the middle s-symbol in \
\\\"word\\\" if it is palindromic and represents a reflection.\>\""}], 
  ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{
   RowBox[{"ConjugatingElement", "::", "usage"}], "=", 
   "\"\<CentralGenerator[\\\"word\\\"] returns the subword of \\\"word\\\" \
such that \\\"word\\\" is the conjugate of some generator by that \
subword.\>\""}], ";"}]}], "Input",
 CellChangeTimes->{{3.8555795462455516`*^9, 3.8555795558298225`*^9}, {
