Cell[BoxData[{
 RowBox[{
  RowBox[{
   RowBox[{"IrreducibleFactor", "::", "usage"}], "=", 
   "\"\<IrreducibleFactor[M,s] returns the list of all vertices in the \
connected component of the Coxeter diagram of type M.\>\""}], ";", 
  RowBox[{"(*", 
   RowBox[{
   "This", " ", "function", " ", "privately", " ", "also", " ", "takes", " ", 
    "inputs", " ", "of", " ", "the", " ", 
    RowBox[{"form", " ", "[", 
     RowBox[{"M", ",", 
      RowBox[{"{", 
       RowBox[{"s1", ",", "s2", ",", "..."}], "}"}]}], "]"}], " ", "but", " ",
     "assumes", " ", "a", " ", "priori", " ", "that", " ", 
    RowBox[{"{", 
     RowBox[{"s1", ",", "s2", ",", "..."}], "}"}], " ", "all", " ", "lie", 
    " ", "in", " ", "the", " ", "same", " ", "connected", " ", 
    RowBox[{"component", "."}]}], "*)"}], "\[IndentingNewLine]", 
  RowBox[{
   RowBox[{"IrreducibleCGQ", "::", "usage"}], "=", 
   "\"\<IrreducibleCGQ[M] returns True if M represents an irreducible Coxeter \
system, and False otherwise.\>\""}], ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{
   RowBox[{"SpecialSubgroup", "::", "usage"}], "=", 
   "\"\<SpecialSubgroup[M,{s1,s2,...}] returms the pair {N,sList}, where \
sList=Sort[{s1,s2,...}], and N is the Coxeter matrix of the special subgroup \
generated by sList.\>\""}], ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{
   RowBox[{"ConvertToSpecialSubgroup", "::", "usage"}], "=", 
   "\"\<ConvertToSpecialSubgroup[M,{N,sList},w] rewrites the word w which is \
an element of the special subgroup W(N) of W(M), which is expressed in the \
generators of W(M), as a word in the generators \
{\\\"s1\\\",...,\\\"sLength[N]\\\"} which have been identified with the \
elements of sList.\nConvertToSpecialSubgroup[M,{N,sList},wList] rewrites each \
element in wList in terms of the new generators of W(N).\>\""}], 
  ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{
   RowBox[{"ConvertFromSpecialSubgroup", "::", "usage"}], "=", 
   "\"\<ConvertFromSpecialSubgroup[M,{N,sList},w] applies the inverse \
operation to ConvertToSpecialSubgroup[M,{N,sList},w].\n\
ConvertFromSpecialSubgroup[M,{N,sList},wList] applies the inverse operation \
to ConvertToSpecialSubgroup[M,{N,sList},wList].\>\""}], 
  ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{
   RowBox[{"IrreducibleFactors", "::", "usage"}], "=", 
   "\"\<IrreducibleFactors[M] returs a list of the sets of generators in each \
irreducible special subgroup of W(M).\>\""}], ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{
   RowBox[{"IrreducibleSpecialSubgroups", "::", "usage"}], "=", 
   "\"\<IrreducibleSpecialSubgroups[M] returns a list of all the irreducible \
special subgroups of W(M), each in the form {N,sList} where N is the Coxeter \
matrix, and sList is the list of generators for W(N).\>\""}], 
  ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{
   RowBox[{"SpecialSubgroups", "::", "usage"}], "=", 
   "\"\<SpecialSubgroups[M] returns a list of all non-trivial special \
subgroups of the Coxeter system with the Coxeter matrix M.\>\""}], 
  ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{
   RowBox[{"SphericalSubgroups", "::", "usage"}], "=", 
   "\"\<SpericalSubgroups[M] returns a list of all non-trivial finite special \
subgroups of the Coxeter system with the Coxeter matrix M.\>\""}], 
  ";"}]}], "Input",
